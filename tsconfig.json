{
    "compilerOptions": {
        "target": "es5",
        "module": "CommonJS",
        "lib": [
            "es6",
            "dom",
        ],
        "allowJs": true,
        "outDir": "lib",
        "rootDir": "src",
        "strict": false,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}